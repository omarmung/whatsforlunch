{
	"name": "What's For Lunch?",
	"version": "1.7",
	"description": "Why don't you eat something.",
	"homepage_url": "http://go/WhatsForLunch",
	"update_url": "http://go/chrome-ext/update",
	"icons": 
			{ 
			"16" : "images/icon.png",
			"48" : "images/48.png",
			"128" : "images/128.png"
			},
	"homepage_url": "http://go/whatsforlunch",
