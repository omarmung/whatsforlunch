			"default_icon": {                    
			  "19": "images/icon.png",           
			  "38": "images/icon.png"            
			},
			"default_title": "What's for Lunch?",
			"default_popup": "popup.html"        
  },
  	"web_accessible_resources": ["scripts/popup.js", "scripts/widgets.js"],
